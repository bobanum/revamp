{
    "name": "bobanum/revamp",
    "type": "library",
    "description": "Description of your extension",
    "keywords": ["laravel", "extension", "bobanum", "revamp"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bobanum\\Revamp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bobanum\\Revamp\\RevampServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
