{
    "name": "bobanum/revamp",
    "version": "0.1.0",
    "description": "Laravel utility to regroup and reorganize your routes, controllers, models, views... into concepts",
    "keywords": [
        "laravel",
        "concepts"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/bobanum/revamp/issues",
        "source": "https://github.com/bobanum/revamp"
    },
    "authors": [
        {
            "name": "Martin Boudreau",
            "email": "bobanum@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bobanum\\Revamp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bobanum\\Revamp\\RevampServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {}
}